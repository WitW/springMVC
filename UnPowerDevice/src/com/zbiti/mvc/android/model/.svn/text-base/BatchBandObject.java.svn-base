package com.zbiti.mvc.android.model;

import java.util.HashMap;

/**
 * 批量绑定对象
 * @author dlt
 *
 */
public class BatchBandObject {
	private String staffId;
	
	private String area ;	
	
	private String obj ; //端口/插头
	
	private String opr ; //绑定/解绑/巡检
	
	private String deviceRfid;	//设备RFID

	private HashMap<String,String> relas; //对应关系 Map<端口ID,RFID>
	
	private HashMap<String,String> relasS; //模块RFID
	
	private String deviceId;	//设备ID
	
	private String longitude; //经度
	
	private String latitude; //纬度
	
	public String getStaffId() {
		return staffId;
	}

	public void setStaffId(String staffId) {
		this.staffId = staffId;
	}

	public String getArea() {
		return area;
	}

	public void setArea(String area) {
		this.area = area;
	}

	public String getObj() {
		return obj;
	}

	public void setObj(String obj) {
		this.obj = obj;
	}

	public String getOpr() {
		return opr;
	}

	public void setOpr(String opr) {
		this.opr = opr;
	}

	public String getDeviceRfid() {
		return deviceRfid;
	}

	public void setDeviceRfid(String deviceRfid) {
		this.deviceRfid = deviceRfid;
	}

	public HashMap<String, String> getRelas() {
		return relas;
	}

	public void setRelas(HashMap<String, String> relas) {
		this.relas = relas;
	}

	public HashMap<String, String> getRelasS() {
		return relasS;
	}

	public void setRelasS(HashMap<String, String> relasS) {
		this.relasS = relasS;
	}

	public String getDeviceId() {
		return deviceId;
	}

	public void setDeviceId(String deviceId) {
		this.deviceId = deviceId;
	}

	public String getLongitude() {
		return longitude;
	}

	public void setLongitude(String longitude) {
		this.longitude = longitude;
	}

	public String getLatitude() {
		return latitude;
	}

	public void setLatitude(String latitude) {
		this.latitude = latitude;
	}
	
}
